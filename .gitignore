# Gitignore for Node.js projects and Vercel/Netlify specific files

# Node modules
node_modules/

# Environment variables
.env

# Vercel / Netlify build output and cache
.vercel/
.netlify/

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.log

# Mac specific files
.DS_Store

# VS Code specific files
.vscode/

# Build artifacts
dist/
build/
out/

# Other common ignored files
*.zip
*.tar
*.tgz
