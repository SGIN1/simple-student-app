{
  "rewrites": [
    {
      "source": "/",
      "destination": "/بيانات_الطلاب.html"
    },
    {
      "source": "/students",
      "destination": "/بيانات_الطلاب.html"
    },
    {
      "source": "/add-student",
      "destination": "/إضافة_طالب.html"
    },
    {
      "source": "/edit-student",
      "destination": "/تعديل_الطالب.html"
    },
    {
      "source": "/edit-student/:id",
      "destination": "/تعديل_الطالب.html"
    },
    {
      "source": "/api/addStudent",
      "destination": "/api/addStudent.js"
    },
    {
      "source": "/api/getStudent",
      "destination": "/api/getStudent.js"
    },
    {
      "source": "/api/updateStudent",
      "destination": "/api/updateStudent.js"
    },
    {
      "source": "/api/deleteStudent",
      "destination": "/api/deleteStudent.js"
    },
    {
      "source": "/api/generateCertificateOne1",
      "destination": "/api/generateCertificateOne1.js"
    },
    {
      "source": "/api/generateCertificateTwo2",
      "destination": "/api/generateCertificateTwo2.js"
    },
    {
      "source": "/api/generateCertificateTwo2/:id",
      "destination": "/api/generateCertificateTwo2.js"
    },
    {
      "source": "/certificate/:id",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ]
}